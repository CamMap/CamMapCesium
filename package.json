{
  "name": "cesium-thales-app",
  "version": "0.3.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "npx webpack serve --mode development --config webpack.config.js --host 0.0.0.0",
    "full": "docker-compose -f docker/debug/docker-compose.yml up -d && npm run start",
    "fullstop": "docker-compose -f docker/debug/docker-compose.yml down",
    "build": "if [ ! -z $PRODUCTION ]; then if [ \"$PRODUCTION\" = true ]; then npx webpack --config webpack.config.release.js; fi; else npx webpack --config webpack.config.js; fi",
    "build:production": "npx webpack --config webpack.config.release.js",
    "docs": "npx typedoc --out docs src",
    "lint": "npx eslint ./src/ts --ext .ts",
    "quicktestbuild": "npx tsc -p tsconfig.json --outdir src/jsfiles",
    "test": "npx webpack --config webpack.config.test.js && npx karma start karma.conf.js --browsers ChromeHeadlessNoSandbox"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "cesium": "^1.75.0",
    "exifr": "^6.0.0"
  },
  "devDependencies": {
    "@types/jest": "^26.0.15",
    "@typescript-eslint/eslint-plugin": "^4.7.0",
    "@typescript-eslint/parser": "^4.7.0",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^6.3.1",
    "css-loader": "^5.0.1",
    "eslint": "^7.13.0",
    "eslint-plugin-jsdoc": "^30.7.8",
    "eslint-plugin-tsdoc": "^0.2.7",
    "glob": "^7.1.6",
    "html-webpack-plugin": "^5.0.0-beta.6",
    "jest": "^26.6.3",
    "json-loader": "^0.5.7",
    "karma": "^5.2.3",
    "karma-chrome-launcher": "^3.1.0",
    "karma-cli": "^2.0.0",
    "karma-coverage": "^2.0.3",
    "karma-jasmine": "^4.0.1",
    "karma-sourcemap-loader": "^0.3.8",
    "karma-typescript": "^5.2.0",
    "path": "^0.12.7",
    "style-loader": "^2.0.0",
    "ts-jest": "^26.4.4",
    "ts-loader": "^8.0.11",
    "typedoc": "^0.19.2",
    "typescript": "^4.0.5",
    "url-loader": "^4.1.1",
    "webpack": "^5.4.0",
    "webpack-cli": "^4.2.0",
    "webpack-dev-server": "^3.11.0"
  }
}
